{
  "name": "langtail",
  "version": "0.3.1-beta.3",
  "description": "",
  "main": "./dist/LangtailNode.js",
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "vitest",
    "ts": "tsc --noEmit",
    "format": "prettier --write .",
    "build": "tsup",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "openai",
    "langtail",
    "nlp",
    "ai",
    "natural language processing",
    "gpt-3",
    "gpt-4",
    "anthropic"
  ],
  "authors": [
    "Langtail <hi@langtail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.12.8",
    "dotenv": "^16.4.5",
    "nock": "14.0.0-beta.5",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.3"
  },
  "module": "./dist/LangtailNode.mjs",
  "types": "./dist/LangtailNode.d.ts",
  "exports": {
    ".": {
      "require": "./dist/LangtailNode.js",
      "import": "./dist/LangtailNode.mjs",
      "types": "./dist/LangtailNode.d.ts"
    },
    "./dist/getOpenAIBody": {
      "require": "./dist/getOpenAIBody.js",
      "import": "./dist/getOpenAIBody.mjs",
      "types": "./dist/getOpenAIBody.d.ts"
    },
    "./dist/template": {
      "require": "./dist/template.js",
      "import": "./dist/template.mjs",
      "types": "./dist/template.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "@langtail/handlebars-evalless": "^0.0.1",
    "date-fns": "^3.6.0",
    "handlebars": "^4.7.8",
    "openai": "^4.40.0",
    "query-string": "^9.0.0",
    "zod": "^3.23.5"
  },
  "tsup": {
    "dts": true,
    "sourcemap": true,
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "entryPoints": [
      "src/LangtailNode.ts",
      "src/template.ts",
      "src/getOpenAIBody.ts"
    ]
  }
}
