{
  "name": "langtail",
  "version": "0.5.2",
  "description": "",
  "main": "./Langtail.js",
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "langtail": "./bin/entry.mjs"
  },
  "scripts": {
    "test": "vitest",
    "ts": "tsc --noEmit",
    "format": "prettier --write .",
    "build": "pnpm run clean && tsup && copyfiles -u 1 src/bin/*.template dist/ && node src/buildHelpers/moveFiles.js",
    "clean": "node src/buildHelpers/cleanFiles.js",
    "prepublishOnly": "pnpm run build",
    "postpublish": "pnpm run clean"
  },
  "keywords": [
    "openai",
    "langtail",
    "nlp",
    "ai",
    "natural language processing",
    "gpt-3",
    "gpt-4",
    "openrouter",
    "anthropic"
  ],
  "authors": [
    "Langtail <hi@langtail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@testing-library/dom": "^10.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.3",
    "copyfiles": "^2.4.1",
    "fs-extra": "^11.2.0",
    "jsdom": "^24.1.0",
    "nock": "14.0.0-beta.5",
    "prettier": "^3.2.5",
    "react-dom": "^18.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "module": "./Langtail.mjs",
  "types": "./Langtail.d.ts",
  "exports": {
    ".": {
      "require": "./Langtail.js",
      "import": "./Langtail.mjs",
      "types": "./Langtail.d.ts"
    },
    "./getOpenAIBody": {
      "require": "./getOpenAIBody.js",
      "import": "./getOpenAIBody.mjs",
      "types": "./getOpenAIBody.d.ts"
    },
    "./template": {
      "require": "./template.js",
      "import": "./template.mjs",
      "types": "./template.d.ts"
    },
    "./openai": {
      "require": "./openai.js",
      "import": "./openai.mjs",
      "types": "./openai.d.ts"
    },
    "./stream": {
      "require": "./stream/index.js",
      "import": "./stream/index.mjs",
      "types": "./stream/index.d.ts"
    },
    "./react/useChatStream": {
      "require": "./react/useChatStream.js",
      "import": "./react/useChatStream.mjs",
      "types": "./react/useChatStream.d.ts"
    },
    "./vercel-ai": {
      "require": "./vercel-ai/index.js",
      "import": "./vercel-ai/index.mjs",
      "types": "./vercel-ai/index.d.ts"
    },
    "./customTypes": {
      "require": "./customTypes.js",
      "import": "./customTypes.mjs",
      "types": "./customTypes.d.ts"
    }
  },
  "dependencies": {
    "@ai-sdk/provider": "^0.0.5",
    "@ai-sdk/provider-utils": "^0.0.8",
    "@langtail/handlebars-evalless": "^0.1.2",
    "commander": "^12.1.0",
    "date-fns": "^3.6.0",
    "dotenv-flow": "^4.1.0",
    "json-schema-to-zod": "^2.1.0",
    "openai": "^4.43.0",
    "query-string": "^9.0.0",
    "react": "^18.3.1",
    "zod": "^3.23.8"
  },
  "tsup": {
    "dts": true,
    "sourcemap": true,
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "entryPoints": [
      "src/Langtail.ts",
      "src/customTypes.ts",
      "src/template.ts",
      "src/getOpenAIBody.ts",
      "src/openai.ts",
      "src/vercel-ai/index.ts",
      "src/bin/entry.ts",
      "src/stream/index.ts",
      "src/react/useChatStream.ts"
    ],
    "external": [
      "dotenv-flow",
      "langtail/customTypes"
    ]
  }
}