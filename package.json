{
  "name": "langtail",
  "version": "0.4.2",
  "description": "",
  "main": "./dist/LangtailNode.js",
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "langtail": "./dist/bin/entry.mjs"
  },
  "scripts": {
    "test": "vitest",
    "ts": "tsc --noEmit",
    "format": "prettier --write .",
    "build": "tsup && copyfiles -u 1 src/bin/langtailTools.template.ts dist/",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "openai",
    "langtail",
    "nlp",
    "ai",
    "natural language processing",
    "gpt-3",
    "gpt-4",
    "openrouter",
    "anthropic"
  ],
  "authors": [
    "Langtail <hi@langtail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@testing-library/dom": "^10.0.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.3",
    "copyfiles": "^2.4.1",
    "jsdom": "^24.1.0",
    "nock": "14.0.0-beta.5",
    "prettier": "^3.2.5",
    "react-dom": "^18.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "module": "./dist/LangtailNode.mjs",
  "types": "./dist/LangtailNode.d.ts",
  "exports": {
    ".": {
      "require": "./dist/LangtailNode.js",
      "import": "./dist/LangtailNode.mjs",
      "types": "./dist/LangtailNode.d.ts"
    },
    "./dist/getOpenAIBody": {
      "require": "./dist/getOpenAIBody.js",
      "import": "./dist/getOpenAIBody.mjs",
      "types": "./dist/getOpenAIBody.d.ts"
    },
    "./dist/template": {
      "require": "./dist/template.js",
      "import": "./dist/template.mjs",
      "types": "./dist/template.d.ts"
    },
    "./dist/vercelAi": {
      "require": "./dist/vercelAi/index.js",
      "import": "./dist/vercelAi/index.mjs",
      "types": "./dist/vercelAi/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ai-sdk/provider": "^0.0.5",
    "@ai-sdk/provider-utils": "^0.0.8",
    "@langtail/handlebars-evalless": "^0.1.1",
    "commander": "^12.1.0",
    "date-fns": "^3.6.0",
    "dotenv-flow": "^4.1.0",
    "handlebars": "^4.7.8",
    "json-schema-to-zod": "^2.1.0",
    "openai": "^4.43.0",
    "query-string": "^9.0.0",
    "react": "^18.3.1",
    "zod": "^3.23.8"
  },
  "tsup": {
    "dts": true,
    "sourcemap": true,
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true,
    "entryPoints": [
      "src/LangtailNode.ts",
      "src/template.ts",
      "src/getOpenAIBody.ts",
      "src/vercelAi/index.ts",
      "src/bin/entry.ts"
    ],
    "external": [
      "dotenv-flow"
    ]
  }
}